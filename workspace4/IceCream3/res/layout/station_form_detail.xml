<?xml version="1.0" encoding="UTF-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <RelativeLayout
         android:id="@+id/station_detail_layout"
         android:layout_width="match_parent"
         android:layout_height="wrap_content">
        <LinearLayout
             android:id="@+id/station_detail_name"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:orientation="horizontal">
            <TextView
                 android:id="@+id/station_detail_name_descriptor"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="@string/station_detail_name_descriptor"
                 style="@style/station_detail_name_descriptor_style" />
            <TextView
                 android:id="@+id/station_detail_name_value"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 style="@style/station_detail_name_value_style" />
        </LinearLayout>
        <LinearLayout
             android:id="@+id/station_detail_meanactualvalue"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_below="@+id/station_detail_name">
            <TextView
                 android:id="@+id/station_detail_meanactualvalue_descriptor"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="@string/station_detail_meanactualvalue_descriptor"
                 style="@style/station_detail_meanactualvalue_descriptor_style" />
            <TextView
                 android:id="@+id/station_detail_meanactualvalue_value"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 style="@style/station_detail_meanactualvalue_value_style" />
        </LinearLayout>
        <LinearLayout
             android:id="@+id/station_detail_meanvariancevalue"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_below="@+id/station_detail_meanactualvalue">
            <TextView
                 android:id="@+id/station_detail_meanvariancevalue_descriptor"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="@string/station_detail_meanvariancevalue_descriptor"
                 style="@style/station_detail_meanvariancevalue_descriptor_style" />
            <TextView
                 android:id="@+id/station_detail_meanvariancevalue_value"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 style="@style/station_detail_meanvariancevalue_value_style" />
        </LinearLayout>
        <LinearLayout
             android:id="@+id/station_detail_numberofentries"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_below="@+id/station_detail_meanvariancevalue">
            <TextView
                 android:id="@+id/station_detail_numberofentries_descriptor"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="@string/station_detail_numberofentries_descriptor"
                 style="@style/station_detail_numberofentries_descriptor_style" />
            <TextView
                 android:id="@+id/station_detail_numberofentries_value"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 style="@style/station_detail_numberofentries_value_style" />
        </LinearLayout>
        <LinearLayout
             android:id="@+id/station_detail_target"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_below="@+id/station_detail_numberofentries">
            <TextView
                 android:id="@+id/station_detail_target_descriptor"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="@string/station_detail_target_descriptor"
                 style="@style/station_detail_target_descriptor_style" />
            <TextView
                 android:id="@+id/station_detail_target_value"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 style="@style/station_detail_target_value_style" />
        </LinearLayout>
    </RelativeLayout>
</merge>
