<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
     package="org.quasar.IceCream3"
     android:versionCode="1"
     android:versionName="1.0">
    <uses-sdk
         android:minSdkVersion="14"
         android:targetSdkVersion="17" />
    <uses-permission
         android:name="android.permission.INTERNET" />
    <application
         android:allowBackup="true"
         android:icon="@drawable/ic_launcher"
         android:label="@string/app_name"
         android:theme="@style/AppTheme"
         android:name="IceCreamMemory">
        <activity
             android:name="IceCreamLauncher"
             android:label="@string/app_name">
            <intent-filter>
                <action
                     android:name="android.intent.action.MAIN" />
                <category
                     android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
             android:configChanges="orientation"
             android:name="org.quasar.IceCream3.presentationLayer.Address.AddressActivity"
             android:label="@string/tittle_master_address">
            <meta-data
                 android:name="android.support.PARENT_ACTIVITY"
                 android:value=".IceCreamLauncher" />
        </activity>
        <activity
             android:configChanges="orientation"
             android:name="org.quasar.IceCream3.presentationLayer.CalendarDate.CalendarDateActivity"
             android:label="@string/tittle_master_calendardate">
            <meta-data
                 android:name="android.support.PARENT_ACTIVITY"
                 android:value=".IceCreamLauncher" />
        </activity>
        <activity
             android:configChanges="orientation"
             android:name="org.quasar.IceCream3.presentationLayer.Entry.EntryActivity"
             android:label="@string/tittle_master_entry">
            <meta-data
                 android:name="android.support.PARENT_ACTIVITY"
                 android:value=".IceCreamLauncher" />
        </activity>
        <activity
             android:configChanges="orientation"
             android:name="org.quasar.IceCream3.presentationLayer.Station.StationActivity"
             android:label="@string/tittle_master_station">
            <meta-data
                 android:name="android.support.PARENT_ACTIVITY"
                 android:value=".IceCreamLauncher" />
        </activity>
    </application>
</manifest>
