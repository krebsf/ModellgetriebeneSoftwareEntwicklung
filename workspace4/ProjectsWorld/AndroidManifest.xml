<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
     package="org.quasar.ProjectsWorld"
     android:versionCode="1"
     android:versionName="1.0">
    <uses-sdk
         android:minSdkVersion="11"
         android:targetSdkVersion="23" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission
         android:name="android.permission.INTERNET" />
    <application
         android:allowBackup="true"
         android:icon="@drawable/ic_launcher"
         android:label="@string/app_name"
         android:theme="@style/AppTheme"
         android:name="ProjectsWorldMemory">
        <activity
             android:name="ProjectsWorldLauncher"
             android:label="@string/app_name">
            <intent-filter>
                <action
                     android:name="android.intent.action.MAIN" />
                <category
                     android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
             android:configChanges="orientation"
             android:name="org.quasar.ProjectsWorld.presentationLayer.CalendarDate.CalendarDateActivity"
             android:label="@string/tittle_master_calendardate">
            <meta-data
                 android:name="android.support.PARENT_ACTIVITY"
                 android:value=".ProjectsWorldLauncher" />
        </activity>
        <activity
             android:configChanges="orientation"
             android:name="org.quasar.ProjectsWorld.presentationLayer.Company.CompanyActivity"
             android:label="@string/tittle_master_company">
            <meta-data
                 android:name="android.support.PARENT_ACTIVITY"
                 android:value=".ProjectsWorldLauncher" />
        </activity>
        <activity
             android:configChanges="orientation"
             android:name="org.quasar.ProjectsWorld.presentationLayer.Member.MemberActivity"
             android:label="@string/tittle_master_member">
            <meta-data
                 android:name="android.support.PARENT_ACTIVITY"
                 android:value=".ProjectsWorldLauncher" />
        </activity>
        <activity
             android:configChanges="orientation"
             android:name="org.quasar.ProjectsWorld.presentationLayer.Project.ProjectActivity"
             android:label="@string/tittle_master_project">
            <meta-data
                 android:name="android.support.PARENT_ACTIVITY"
                 android:value=".ProjectsWorldLauncher" />
        </activity>
        <activity
             android:configChanges="orientation"
             android:name="org.quasar.ProjectsWorld.presentationLayer.Qualification.QualificationActivity"
             android:label="@string/tittle_master_qualification">
            <meta-data
                 android:name="android.support.PARENT_ACTIVITY"
                 android:value=".ProjectsWorldLauncher" />
        </activity>
        <activity
             android:configChanges="orientation"
             android:name="org.quasar.ProjectsWorld.presentationLayer.Training.TrainingActivity"
             android:label="@string/tittle_master_training">
            <meta-data
                 android:name="android.support.PARENT_ACTIVITY"
                 android:value=".ProjectsWorldLauncher" />
        </activity>
        <activity
             android:configChanges="orientation"
             android:name="org.quasar.ProjectsWorld.presentationLayer.Worker.WorkerActivity"
             android:label="@string/tittle_master_worker">
            <meta-data
                 android:name="android.support.PARENT_ACTIVITY"
                 android:value=".ProjectsWorldLauncher" />
        </activity>
    </application>
</manifest>
